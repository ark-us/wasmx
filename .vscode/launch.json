{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    {
        "name": "mythos-Attach to Process",
        "type": "go",
        "request": "attach",
        "mode": "local",
        "processId":"${command:pickProcess}"
        // "processName":"mythosd"
    },
        {
            "name": "mythos",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            // "args": ["-test.timeout", "30m", "-test.v"],
            "args": ["start"],
            // "buildFlags": "-tags e2e",
            "env": {
                "WASMEDGE_DIR": "/Users/user/.wasmedge",
                "DYLD_LIBRARY_PATH": "/Users/user/.wasmedge/lib",
                "LD_LIBRARY_PATH": "/Users/user/.wasmedge/lib",
                "LIBRARY_PATH": "/Users/user/.wasmedge/lib",
                "WASMEDGE_LIB_DIR": "/Users/user/.wasmedge/lib",
                "C_INCLUDE_PATH": "/Users/user/.wasmedge/include",
                "CPLUS_INCLUDE_PATH": "/Users/user/.wasmedge/include",
                "CGO_LDFLAGS": "-Wl,-rpath,/Users/user/.wasmedge/lib",
                "GOROOT": "/usr/local/go",
                "GOPATH": "/Users/user/go",
            }
        }
    ]
}
